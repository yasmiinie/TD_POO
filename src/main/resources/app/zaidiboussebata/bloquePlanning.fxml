<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="250.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.zaidiboussebata.Control.addSchedule">
   <children>
      <Text fill="#17207f" layoutX="32.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Do you want to block the slot for this task">
         <font>
            <Font name="Verdana Bold" size="18.0" />
         </font>
      </Text>
      <Button fx:id="refuseBut" layoutX="266.0" layoutY="138.0" mnemonicParsing="false" onAction="#refuseClick" prefHeight="37.0" prefWidth="171.0" style="-fx-background-color: #069CAB; -fx-background-radius: 7;" text="Refuse" textFill="WHITE">
         <font>
            <Font name="Verdana Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="acceptBut" layoutX="55.0" layoutY="138.0" mnemonicParsing="false" onAction="#acceptClick" prefHeight="37.0" prefWidth="171.0" style="-fx-background-color: #F69CA7; -fx-background-radius: 7;" text="Accept" textFill="WHITE">
         <font>
            <Font name="Verdana Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
